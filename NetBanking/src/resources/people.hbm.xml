<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.netbanking.people.People" table="people" >
    <id name="id" column = "id"></id>
    <property name="name" column="name"></property>
    <property name="surname" column="surname"></property>
    <property name="age" column="age"></property>
    <property name="address" column="address"></property>
    <property name="mobile_number" column="mobile_number"></property>
    <property name="people_type" column="people_type"></property>
    
    </class>	
    
    </hibernate-mapping>